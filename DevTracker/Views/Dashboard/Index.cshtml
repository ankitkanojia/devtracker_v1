@model DashboardVm

@{
    ViewBag.Title = "Index";
    var roleMasterId = Convert.ToInt64(CookieHelper.GetCookie(CookieName.RoleMasterId));
    var isOwnerPm = roleMasterId == (int) EnumList.Roles.Owner || roleMasterId == (int) EnumList.Roles.Project_Manager;
}

<!--Page Header-->
<div class="header">
    <div class="header-content">
        <div class="page-title"><i class="icon-store2"></i> Dashboard</div>
    </div>
</div>
<!-- /Page header -->

<div class="container-fluid page-content">

@if (isOwnerPm)
{
    <div class="row">
        <div class="col-md-3">
            <div class="panel panel-flat bg-purple-a200">
                <div class="panel-heading">
                    <h5 class="text-uppercase text-semibold text-alpha-4">Total Resources</h5>
                </div>
                <div class="panel-body">
                    <div class="row m-b-10">
                        <div class="col-md-3 col-sm-3 col-xs-3">
                            <i class="icon-users2 x5 text-alpha-2"></i>
                        </div>
                        <div class="col-md-9 col-sm-9 col-xs-9">
                            <div class="x6 text-light no-line-height text-alpha-9 text-right">
                                @Model.TotalResource
                                <i class="icon-arrow-up16 x2 text-alpha-6 position-right"></i>
                            </div>
                        </div>
                    </div>
                    <h5 class="text-alpha-5 pull-left no-margin-b">Project Manager(s)</h5>
                    <h5 class="text-bold text-alpha-7 pull-right">
                        @Model.TprojectManager
                    </h5>
                    <div class="clearfix"></div>
                    <h5 class="text-alpha-5 pull-left">Team Leader(s)</h5>
                    <h5 class="text-bold text-alpha-7 pull-right">
                        @Model.TteamLeader
                    </h5>
                    <div class="clearfix"></div>
                    <h5 class="text-alpha-5 pull-left no-margin-b">Developer(s)</h5>
                    <h5 class="text-bold text-alpha-7 pull-right">
                        @Model.Tdesigner
                    </h5>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>

        <div class="col-md-3">
            <div class="panel panel-flat bg-amber-600">
                <div class="panel-heading">
                    <h5 class="text-uppercase text-semibold text-alpha-8">Working Resource</h5>
                </div>
                <div class="panel-body">
                    <div class="row m-b-10">
                        <div class="col-md-3 col-sm-3 col-xs-3">
                            <i class="icon-user-check x5 text-alpha-4"></i>
                        </div>
                        <div class="col-md-9 col-sm-9 col-xs-9">
                            <div class="x6 text-light no-line-height text-right">1,264</div>
                        </div>
                    </div>
                    <h5 class="text-alpha-5 pull-left no-margin-b">Project Manager(s)</h5>
                    <h5 class="text-bold text-alpha-7 pull-right">240</h5>
                    <div class="clearfix"></div>
                    <h5 class="text-alpha-5 pull-left">Team Leader(s)</h5>
                    <h5 class="text-bold text-alpha-7 pull-right">3%</h5>
                    <div class="clearfix"></div>
                    <h5 class="text-alpha-5 pull-left no-margin-b">Developer(s)</h5>
                    <h5 class="text-bold text-alpha-7 pull-right">240</h5>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>

        <div class="col-md-3">
            <div class="panel panel-flat bg-info-a700">
                <div class="panel-heading">
                    <h5 class="text-uppercase text-semibold text-alpha-8">Free Resource</h5>
                </div>
                <div class="panel-body">
                    <div class="row m-b-10">
                        <div class="col-md-3 col-sm-3 col-xs-3">
                            <i class="icon-user-minus x5 text-alpha-4"></i>
                        </div>
                        <div class="col-md-9 col-sm-9 col-xs-9">
                            <div class="x5 text-light no-line-height text-right">
                                <small class="x2 position-left">$</small>40,268
                            </div>
                        </div>
                    </div>
                    <h5 class="text-alpha-5 pull-left no-margin-b">Project Manager(s)</h5>
                    <h5 class="text-bold text-alpha-7 pull-right">240</h5>
                    <div class="clearfix"></div>
                    <h5 class="text-alpha-5 pull-left">Team Leader(s)</h5>
                    <h5 class="text-bold text-alpha-7 pull-right">3%</h5>
                    <div class="clearfix"></div>
                    <h5 class="text-alpha-5 pull-left no-margin-b">Developer(s)</h5>
                    <h5 class="text-bold text-alpha-7 pull-right">240</h5>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>

        <div class="col-md-3">
            <div class="panel panel-flat bg-teal-700">
                <div class="panel-heading">
                    <h5 class="text-uppercase text-semibold text-alpha-8">Total projects</h5>
                </div>
                <div class="panel-body">
                    <div class="row m-b-10">
                        <div class="col-md-3 col-sm-3 col-xs-3">
                            <i class="icon-briefcase x5 text-alpha-4"></i>
                        </div>
                        <div class="col-md-9 col-sm-9 col-xs-9">
                            <div class="x5 text-light no-line-height text-right">
                                <small class="x2 position-left">$</small>8,420
                            </div>
                        </div>
                    </div>
                    <h5 class="text-alpha-7 pull-left no-margin-b">This month</h5>
                    <h5 class="text-bold text-alpha-9 pull-right">$485</h5>
                    <div class="clearfix"></div>
                    <h5 class="text-alpha-7 pull-left">Last month</h5>
                    <h5 class="text-bold text-alpha-9 pull-right">$980</h5>
                    <div class="clearfix"></div>
                    <h5 class="text-alpha-7 pull-left">This year</h5>
                    <h5 class="text-bold text-alpha-9 pull-right">$980</h5>
                    <div class="clearfix"></div>
                </div>
            </div>
        </div>
    </div>
}


